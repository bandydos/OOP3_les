<Window x:Class="BubblingDemo_070220.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BubblingDemo_070220"
        mc:Ignorable="d"
        Title="Bubbling" Height="450" Width="800" FontSize="22"
        Name="wndwMain"
        MouseDown="lblBubbling_MouseDown">
    <StackPanel x:Name="stckpnlBubbling" MouseDown="lblBubbling_MouseDown">
        <GroupBox x:Name="grpbxBubbling" Header="Klik om te starten" MouseDown="lblBubbling_MouseDown">
            <Label Name="lblBubbling" Margin="10" MouseDown="lblBubbling_MouseDown">Bubbling</Label>
        </GroupBox>
        <GroupBox Header="Info">
            <TextBox x:Name="tbxInfo"></TextBox>
        </GroupBox>
    </StackPanel>
</Window>
