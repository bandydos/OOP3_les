<Window x:Class="TunnelingDemo_070220.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TunnelingDemo_070220"
        mc:Ignorable="d"
        Title="Tunneling" Height="450" Width="800"
        FontSize="22"
        Name="wndwMain"
        PreviewMouseDown="lblTunneling_PreviewMouseDown">
    <StackPanel x:Name="stckpnlTunneling" PreviewMouseDown="lblTunneling_PreviewMouseDown">
        <GroupBox x:Name="grpbxTunneling" Header="Klik om te starten" PreviewMouseDown="lblTunneling_PreviewMouseDown">
            <Label Name="lblTunneling" Margin="10" PreviewMouseDown="lblTunneling_PreviewMouseDown">Neerdalen</Label>
        </GroupBox>
        <GroupBox Header="Info">
            <TextBox x:Name="tbxInfo"></TextBox>
        </GroupBox>
    </StackPanel>
</Window>
